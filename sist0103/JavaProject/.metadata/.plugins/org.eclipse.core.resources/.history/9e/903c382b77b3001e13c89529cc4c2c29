package day0115;

import java.util.Date;

public class NullPointExeption {
	Date date; //선언
	public void write() {
		int y,m,d;
		y= date.getYear()-1900;
		m= date.getMonth()+1;
		d= date.getDate();
		
		System.out.println(y+"년"+ m+"월"+d+"일 입니다.");
	}

	public static void main(String[] args) {
		NullPointerException
	}

}
