package day0112;

//부모클래스
class Color{
	
	public void hello() {
		System.out.println("안녕하세용~~~~");
	}
}
/////////////////////////////////////////
class Red extends Color{
	public void process() {
		System.out.println("나의 차는 빨강색이야");
	}
}
/////////////////////////////////////////////
class Green extends Color{
	public void process() {
		System.out.println("나의 차는 그린색이야");
	}
}
/////////////////////////////////////////////
class Blue extends Color{
	public void process() {
		System.out.println("나의 차는 파랑색이야");
	}
}
/////////////////////////////////////////////
class Pink extends Color{
	public void process() {
		System.out.println("나의 차는 핑크색이야");
	}
}
/////////////////////////////////////////////







public class Inherit_09 {

	public static void main(String[] args) {
		
		//자식클래스 extends 부모
		System.out.println("111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111111");
	}

}
