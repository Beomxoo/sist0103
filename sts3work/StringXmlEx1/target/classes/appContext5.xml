<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">
	
	<bean id="info" class="spring.di.ex5.SawonInfo">
		<constructor-arg value="김민규"/>
		<constructor-arg value="일반사원"/>
		<property name="pay" value="1300000"/>
		<property name="buseo">
			<list>
				<value>디자인부</value>
				<value>프론트부</value>
				<value>백엔드부</value>
				<value>전산관리부</value>
				<value>현장감독부</value>
			</list>
		</property>
	</bean>
	
	<bean id="emp" class="spring.di.ex5.Emp">
		<constructor-arg value="쌍용교육센터"/>
		<property name="emploc" value="서울시 강남구 한독빌딩"/>
		<property name="sawoninfo" ref="info"/>
	</bean>

</beans>
