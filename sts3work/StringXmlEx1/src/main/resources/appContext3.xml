<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<!-- SawonData bean 설정... 사원명은 생성자주입,급여,입사일은 setter 주입 -->
	<bean id="sawondata" class="spring.di.ex3.SawonData">
		<constructor-arg value="김민규"/>
		<property name="pay" value="5000000"></property>
		<property name="ipsaday" value="2024-03-30"></property>
	</bean>
	
	<bean name="sdata2" class="spring.di.ex3.SawonData">
		<property name="sawonname" value="김만규"></property>
		<property name="pay" value="7000000"></property>
		<property name="ipsaday" value="2024-03-20"></property>
	</bean>
	<!-- Sawon bean설정 -->
	<bean id="sawon" class="spring.di.ex3.Sawon">
		<constructor-arg ref="sawondata"/>
	</bean>
	<bean id="sawon2" class="spring.di.ex3.Sawon">
		<constructor-arg ref="sdata2"/>
	</bean>
	
	
</beans>
